<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/verdeZup"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:layout_width="150dp"
            android:layout_height="175dp"
            android:layout_gravity="center"
            android:contentDescription=""
            android:src="" />

        <EditText
            android:id="@+id/etUsuario"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:backgroundTint="@color/amareloZup"
            android:hint=""
            android:padding="16dp"
            android:textColorHint="@color/amareloZup" />

        <EditText
            android:id="@+id/etSenha"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:backgroundTint="@color/amareloZup"
            android:hint=""
            android:inputType="textPassword"
            android:padding="16dp"
            android:textColorHint="@color/amareloZup" />

        <EditText
            android:id="@+id/etInfluencer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:backgroundTint="@color/amareloZup"
            android:hint=""
            android:padding="16dp"
            android:textColorHint="@color/amareloZup" />

        <EditText
            android:id="@+id/etFoto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:backgroundTint="@color/amareloZup"
            android:hint=""
            android:inputType="text"
            android:padding="16dp"
            android:textColorHint="@color/amareloZup" />

        <CheckBox
            android:id="@+id/aceitarCheckBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text=""
            android:textColor="@color/amareloZup" />

        <Button
            android:id="@+id/cadastrarButton"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:backgroundTint="@color/amareloZup"
            android:text=""
            android:textColor="@color/verdeZup" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>